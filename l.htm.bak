</div>

<div class="content">
<script>
document.write('<iframe id="main" name="main" frameborder="0" marginheight="0" marginwidth="0" src="');
if (document.location.search.substring(1)!="") {
	if (document.location.search.substring(1).indexOf('&')>=0) {
		document.write (document.location.search.substring(1).replace('&', '?'));
	} else {
		document.write (document.location.search.substring(1));
	}
} else {
	document.write ('home.htm');
}
document.write('" width="100%" height="100%"></iframe>');
</script>
</div><!-- end content 2 -->

<!-- Footer -->

<div class="content">
